{
    "name": "seankndy/fluent-sonar-api",
    "description": "A Sonar Software (https://sonar.software) API client with a fluent interface.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sean Kennedy",
            "email": "sean@kndy.net"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "guzzlehttp/guzzle": ">=6.0",
        "illuminate/pagination": ">=5.0",
        "illuminate/support": ">=5.0",
        "gmostafa/php-graphql-client": "^1.13"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.10"
    },
    "autoload": {
        "psr-4": {
            "SeanKndy\\SonarApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SeanKndy\\SonarApi\\Tests\\": "tests/"
        }
    }
}
